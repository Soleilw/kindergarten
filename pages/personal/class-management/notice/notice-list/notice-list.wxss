
.list {
    /* padding: 0 0 40rpx 40rpx; */
    padding: 0 0 0 40rpx;
    background-color: #fff;
}

.tableviewcell {
    position: relative;
    padding: 25rpx 25rpx 25rpx 0;
    border-bottom: 2rpx solid #dcdcdc;
    background-color: #fff;
    color: #666;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 35rpx;
}

.tableviewcell .left-icon {
    width: 40rpx;
    height: 40rpx;
    object-fit: cover;
}

.tableviewcell .right-icon {
    width: 50rpx;
    height: 50rpx;
    margin: 0 0 0 10rpx;
}

/*text*/

.tableviewcell rich-text {
    width: 500rpx;
    font-size: 30rpx;
    /* margin-left: 14rpx; */
}

.tableviewcell button {
    width: 120rpx;
    font-size: 24rpx;
}

