/* 提交审核 */
.btn-box{
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.adopt{
  background-color:#2a9f93;
}
.not-through{
  background-color:#f15656;
}
button{
  width:250rpx;
  height:70rpx;
  line-height:70rpx;
  margin:100rpx auto 50rpx auto;
  color:#fff;
  background-color:#2a9f93;
  border-radius: 50rpx;
  font-size: 35rpx;
}
button::after{
  border: none;
}

/* 功能列表 */
.list{
  padding: 0 0 40rpx 40rpx;
  background-color:#fff;
}
/*menu list*/
.tableviewcell {
  position:relative; 
	padding: 25rpx 40rpx 25rpx 0;
	border-bottom: 2rpx solid #dcdcdc;
  background-color:#fff;
	color: #666;
	display: flex;
	align-items: center; 
  justify-content: space-between;
  font-size: 35rpx;
}
.tableviewcell text {
  width:200rpx;
	font-size: 32rpx;
  margin-left: 14rpx;
}

.tableviewcell .value{
  width:400rpx;
  text-align:right;
}
.tableviewcell .left-icon {
	width: 40rpx;
	height: 40rpx;
  object-fit: cover;
}

 .tableviewcell .right-icon {
  /* position:absolute;
  top:30rpx;
  right:40rpx; */
	width: 50rpx;
	height: 50rpx;
  margin:0 0 0 10rpx;
} 

.img {
  padding: 20rpx 0;
  text-align: center;
}

.img image{
  width: 300rpx;
  height: 300rpx;
}

