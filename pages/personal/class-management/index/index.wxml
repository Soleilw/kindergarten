<view class="list">
  <view bindtap="toManage" class="tableviewcell">
    <image class="left-icon" src="/icon/seat2.png" />
    <text>在校信息</text>
    <image class="right-icon" src="/icon/right.png" />
  </view>
  <view bindtap="toAudition" class="tableviewcell">
    <image class="left-icon" src="/icon/examine.png" />
    <text>学生审核</text>
    <image class="right-icon" src="/icon/right.png" />
  </view>
  <view bindtap="toParentAudition" class="tableviewcell" wx:if="{{show}}">
    <image class="left-icon" src="/icon/examine.png" />
    <text>家长审核</text>
    <image class="right-icon" src="/icon/right.png" />
  </view>
  <view bindtap="toNotice" class="tableviewcell">
    <image class="left-icon" src="/icon/news3.png" />
    <text>发布公告</text>
    <image class="right-icon" src="/icon/right.png" />
  </view>
   <view bindtap="toManageNotice" class="tableviewcell">
    <image class="left-icon" src="/icon/classManagement.png" />
    <text>公告管理</text>
    <image class="right-icon" src="/icon/right.png" />
  </view>
</view>