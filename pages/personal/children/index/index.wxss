
.children-list {
  border-bottom: 10rpx solid #eee;
  font-size: 28rpx;
}

.children-list .info {
  position: relative;
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.children-list .info .portrait {
  width: 200rpx;
  height: 200rpx;
  border-radius: 15rpx;
}

.children-list .info .content {
  width: 500rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}

.children-list .info .content view {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.children-list .info .content image {
  width: 30rpx;
  height: 30rpx;
  margin: 0 30rpx;
}

.children-list .info .content text {
  height: 50rpx;
  line-height: 50rpx;
}
.children-list .info .switch {
  position: absolute;
  right:20rpx;
  bottom:20rpx;
  width:170rpx;
  height:60rpx;
  border-radius: 15rpx;
  background-color:#2a9f93;
  color:#fff;
  display: flex;
  justify-content: center;
  align-items: center;
  top: 80rpx;
}
.children-list .info .switch2 {
  position: absolute;
  right:20rpx;
  bottom:20rpx;
  width:170rpx;
  height:60rpx;
  border-radius: 15rpx;
  background-color:#2a9f93;
  color:#fff;
  display: flex;
  justify-content: center;
  align-items: center;
  /* top: 80rpx; */
}
.children-list .info .switch2 image{
  width:25rpx;
  height:25rpx;
  margin:0 10rpx;
}
.children-list .info .switch image{
  width:25rpx;
  height:25rpx;
  margin:0 10rpx;
}
.children-list .family{
   padding:10rpx 0; 
  border-top:3rpx solid #eee;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  font-size: 30rpx;
  color:#666;
}
.children-list .family image{
  width:150rpx;
  height:150rpx;
}
.children-list .family .family-list{
  width:150rpx;
  height:150rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.children-list .family .family-list view{
  position:relative;
  min-width: 100rpx;
  min-height:100rpx;
  width:100rpx;
  height:100rpx;
  border-radius: 50%;
  overflow: hidden;
}
.children-list .family .family-list view text{
  position:absolute;
  left:0;
  top:0;
  width:100%;
  height:100%;
  line-height:100rpx;
  text-align: center;
  font-size: 28rpx;
  color:#fff;
  background-color:rgba(0,0,0,0.5);
}
.children-list .family .family-list .familyName{
  max-width:150rpx;
  height:40rpx;
  display: -webkit-box; 
  -webkit-box-orient: vertical; 
  -webkit-line-clamp: 1; 
  text-overflow: ellipsis; 
  overflow: hidden;
}
.add {
  width: 30vw;
  margin: 20vw auto;
  color: #2a9f93;
  font-size: 4.667vw;
  font-weight: bold;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.add image {
  width: 17.333vw;
  height: 17.333vw;
}
.add view {
  width: 17.333vw;
  height: 17.333vw;
  line-height: 16vw;
  border: 8rpx solid #2a9f93;
  border-radius: 50%;
  font-size: 12vw;
  text-align: center;
}

