/* 提交审核按钮 */
button{
  width:450rpx;
  height:70rpx;
  line-height:70rpx;
  margin:50rpx auto;
  color:#fff;
  background-color:#2a9f93;
  border-radius: 50rpx;
  font-size: 35rpx;
}
button::after{
  border: none;
}

/* 信息列表 */
.list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  color:#666;
}
.list image{
  width:50rpx;
  height:50rpx;
  margin:auto 25rpx;
}
.list view{
  width: 650rpx;
  height:100rpx;
  border-bottom:2rpx solid #ddd;
  display: flex;
  justify-content: space-start;
  align-items: center;
}
.list input{
  width:500rpx;
}
.list text{
 color:#fff;
 width:130rpx;
 height:60rpx;
 line-height:60rpx;
 text-align: center;
 border-radius: 30rpx;
 background-color:#2a9f93;
}
.portrait-box textarea{
  font-size: 30rpx;
  width:100%;
  height:200rpx;
  padding:20rpx;
  border:1rpx solid #eee;
  border-radius: 20rpx;
}


.tableviewcell {
  position: relative;
  padding: 25rpx 40rpx;
  border-bottom: 2rpx solid #dcdcdc;
  background-color: #fff;
  color: #666;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 35rpx;
}

.tableviewcell text {
  width: 200rpx;
  font-size: 32rpx;
  margin-left: 14rpx;
}

.tableviewcell .input {
  display: block;
  width: 400rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  text-align: right;
}

.tableviewcell view {
  width: 400rpx;
  text-align: right;
}

.tableviewcell .left-icon {
  width: 50rpx;
  min-width:50rpx;
  height: 50rpx;
  min-height:50rpx;
  object-fit: cover;
}



