.family{
  padding:50rpx 0;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  font-size: 30rpx;
  color:#666;
}
.family image{
  width:150rpx;
  height:150rpx;
}
.family .list{
  width:250rpx;
  height:230rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.family .list view{
  position:relative;
  width:150rpx;
  height:150rpx;
  border-radius: 50%;
  overflow: hidden;
}
.family .list view text{
  position:absolute;
  left:0;
  top:0;
  width:100%;
  height:100%;
  line-height:150rpx;
  text-align: center;
  color:#fff;
  background-color:rgba(0,0,0,0.5);
}
